<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.io.securityInfrun.web.user2.mapper.User2Mapper">

    <select id="user2InfoSelect" parameterType="uisMap" resultType="uisMap">
         select * from user
    </select>
    
    <insert id="user2InfoInsert">
         insert into user (
			    user_name,
			    password,
			    email,
			    age,
			    role
         		) 
          value (
			    'test','test','test','test','test'
          )
    </insert>
    
</mapper>